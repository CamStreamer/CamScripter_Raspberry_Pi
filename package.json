{
    "name": "camscripter-raspberry",
    "version": "1.2.1",
    "description": "CamScripter Raspberry",
    "repository": {
        "type": "git",
        "url": ""
    },
    "prettier": "@camstreamer/prettier-config",
    "importSort": {
        ".js, .jsx": {
            "parser": "babylon",
            "style": "capital"
        },
        ".ts, .tsx": {
            "parser": "typescript",
            "style": "capital"
        }
    },
    "bin": {
        "camscripter-register": "./bin/register.js",
        "camscripter-unregister": "./bin/unregister.js",
        "camscripter-run": "./bin/run.js"
    },
    "main": "dist/main.js",
    "scripts": {
        "clean": "rimraf dist/*",
        "build": "npm-run-all clean tsc",
        "enviroment": "./prepare_env.sh",
        "tsc": "tsc",
        "start": "ts-node ./src/main.ts",
        "postinstall": "npm run enviroment",
        "register": "./systemd_register.sh",
        "unregister": "./systemd_unregister.sh",
        "postenviroment": "npm run register",
        "pretty": "prettier --write \"./src/*.{ts,tsx}\"",
        "pretty:check": "prettier --check \"./src/*.{ts,tsx}\"",
        "test": "echo \"Error: no test specified\""
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "adm-zip": "^0.5.5",
        "chokidar": "^3.5.2",
        "formidable": "^1.2.2",
        "fs": "0.0.1-security",
        "fs-extra": "^10.0.0",
        "get-port": "^5.1.1",
        "http": "0.0.1-security",
        "http-proxy": "^1.18.1",
        "minimist": "^1.2.5",
        "n-readlines": "^1.0.1",
        "shelljs": "^0.8.4",
        "url": "^0.11.0",
        "ws": "^8.0.0"
    },
    "devDependencies": {
        "@camstreamer/prettier-config": "^2.0.3",
        "@types/adm-zip": "^0.4.34",
        "@types/chokidar": "^2.1.3",
        "@types/formidable": "^1.2.2",
        "@types/fs-extra": "^9.0.11",
        "@types/http-proxy": "^1.17.6",
        "@types/n-readlines": "^1.0.1",
        "@types/node": "^16.4.13",
        "@types/ws": "^7.4.7",
        "import-sort-style-capital": "^1.0.1",
        "import-sort-style-module": "^6.0.0",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.3.2",
        "prettier-plugin-import-sort": "^0.0.7",
        "ts-node": "^10.2.0",
        "typescript": "^4.3.5"
    }
}
